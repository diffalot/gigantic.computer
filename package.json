{
  "dependencies": {
    "three-orbit-controls": "^72.0.0",
    "three": "^0.73.0",
    "choo": "^6.13.1"
  },
  "devDependencies": {
    "chokidar": "^2.0.4",
    "choo-test": "^0.5.0",
    "commander": "^2.19.0",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "esm": "^3.0.84",
    "onchange": "^5.1.3",
    "parcel-bundler": "^1.10.3",
    "postcss-modules": "^1.4.1",
    "pre-commit": "^1.2.2",
    "queue-promise": "^1.3.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1",
    "typescript": "^3.2.1"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "license": "GPL-3.0",
  "author": "Alice Davis <alice@gigantic.computer>",
  "repository": {
    "url": "https://github.com/diffalot/sees.earth.git",
    "type": "git"
  },
  "scripts": {
    "start": "npm run test:watch",
    "//": "// @todo: get types setup for choo, etc",
    "test": "npm run lint && npm run test:unit #&& npm run test:types",
    "test:ci": "npm run test && npm run build",
    "test:watch": "npm run test:nearby | tap-spec",
    "test:nearby": "./bin/spectacle.js --watchPath 'src/**/*.js' --subExtension spec --command 'tape -r esm'",
    "test:unit": "tape -r esm src/**/*.spec.js",
    "test:types": "tsc --noEmit --allowJs --checkJs src/index.js",
    "lint": "eslint src/**/*.js",
    "build": "rm -rf dist && parcel build src/index.html",
    "serve": "parcel src/index.html"
  },
  "main": "index.js",
  "description": "a threejs view of earth",
  "version": "0.0.0",
  "private": "true",
  "name": "gigantic.computer"
}
