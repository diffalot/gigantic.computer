{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "sees-earth"
  },
  "spec": {
    "ports": [{
      "name": "http",
      "protocol": "TCP",
      "port": 80,
      "targetPort": 80
    },{
      "name": "tls",
      "protocol": "TCP",
      "port": 443,
      "targetPort": 443
    }],
    "selector": {
      "run": "sees-earth"
    },
    "type": "LoadBalancer"
  }
}
